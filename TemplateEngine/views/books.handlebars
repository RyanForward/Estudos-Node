<h1>Livros disponíveis</h1>
<div class="books-container">
    <a href="/">Adicionar livro</a>
    <br>
    {{#each books}}
    <div class="book">
    #{{this.id}} - <span class="bold"></span> 
    <a href="/books/{{this.id}}">{{this.title}}</a>- {{this.pageqty}} páginas 
    </div>
    {{/each}}
</div>